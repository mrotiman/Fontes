  object QryListaMembros: TFDQuery
    Connection = dm.Conn
    SQL.Strings = (
      
        'select mb.id_nucleo, mb.id_membro, trim(right(concat('#39'00000'#39',(ca' +
        'st( mb.codigo as char ))),5)) as codigo, mb.nome, '
      
        #9' date_format(mb.dtapresent,'#39'%d/%m/%Y'#39') as dtapresent, nu.nome a' +
        's nomenucleo, '
      #9' st.descricao as situacao '
      '  from membro mb, situacao st, nucleo nu, '
      
        '         (select id_nucleo from nucleoxusuario where id_usuario ' +
        '= 3 ) nxu'
      
        ' where st.id_situacao = mb.id_situacao and nu.id_nucleo = mb.id_' +
        'nucleo'
      '     and nxu.id_nucleo = mb.id_nucleo'
      '  order by mb.nome')
    Left = 496
    Top = 24
  end
  object dsLista: TDataSource
    DataSet = QryListaMembros
    Left = 496
    Top = 72
  end
